#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial-Practice-02/build-fullstack-app-with-nodejs-n-htmx/contact-manager/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial-Practice-02/build-fullstack-app-with-nodejs-n-htmx/contact-manager/node_modules/.pnpm/mime@1.6.0/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial-Practice-02/build-fullstack-app-with-nodejs-n-htmx/contact-manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial-Practice-02/build-fullstack-app-with-nodejs-n-htmx/contact-manager/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial-Practice-02/build-fullstack-app-with-nodejs-n-htmx/contact-manager/node_modules/.pnpm/mime@1.6.0/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial-Practice-02/build-fullstack-app-with-nodejs-n-htmx/contact-manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
