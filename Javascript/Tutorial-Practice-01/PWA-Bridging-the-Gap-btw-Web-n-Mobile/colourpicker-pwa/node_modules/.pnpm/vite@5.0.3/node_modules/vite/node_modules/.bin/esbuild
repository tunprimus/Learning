#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/PWA-Bridging-the-Gap-btw-Web-n-Mobile/colourpicker-pwa/node_modules/.pnpm/esbuild@0.19.8/node_modules/esbuild/bin/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/PWA-Bridging-the-Gap-btw-Web-n-Mobile/colourpicker-pwa/node_modules/.pnpm/esbuild@0.19.8/node_modules/esbuild/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/PWA-Bridging-the-Gap-btw-Web-n-Mobile/colourpicker-pwa/node_modules/.pnpm/esbuild@0.19.8/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/PWA-Bridging-the-Gap-btw-Web-n-Mobile/colourpicker-pwa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/PWA-Bridging-the-Gap-btw-Web-n-Mobile/colourpicker-pwa/node_modules/.pnpm/esbuild@0.19.8/node_modules/esbuild/bin/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/PWA-Bridging-the-Gap-btw-Web-n-Mobile/colourpicker-pwa/node_modules/.pnpm/esbuild@0.19.8/node_modules/esbuild/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/PWA-Bridging-the-Gap-btw-Web-n-Mobile/colourpicker-pwa/node_modules/.pnpm/esbuild@0.19.8/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/PWA-Bridging-the-Gap-btw-Web-n-Mobile/colourpicker-pwa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../../../../esbuild@0.19.8/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
