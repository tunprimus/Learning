#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/Blogging-website-with-pure-HTML-CSS-n-JS/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/Blogging-website-with-pure-HTML-CSS-n-JS/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/Blogging-website-with-pure-HTML-CSS-n-JS/node_modules/.pnpm/touch@3.1.0/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/Blogging-website-with-pure-HTML-CSS-n-JS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/Blogging-website-with-pure-HTML-CSS-n-JS/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/Blogging-website-with-pure-HTML-CSS-n-JS/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/Blogging-website-with-pure-HTML-CSS-n-JS/node_modules/.pnpm/touch@3.1.0/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/Tutorial_Practice/Blogging-website-with-pure-HTML-CSS-n-JS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
