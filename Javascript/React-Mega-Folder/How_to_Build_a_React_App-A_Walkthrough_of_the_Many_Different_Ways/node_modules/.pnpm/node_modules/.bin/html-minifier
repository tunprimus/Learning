#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pr0f33/zzz_personal/Learning/Javascript/React/How_to_Build_a_React_App-A_Walkthrough_of_the_Many_Different_Ways/node_modules/.pnpm/html-minifier@1.5.0/node_modules/html-minifier/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/React/How_to_Build_a_React_App-A_Walkthrough_of_the_Many_Different_Ways/node_modules/.pnpm/html-minifier@1.5.0/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/React/How_to_Build_a_React_App-A_Walkthrough_of_the_Many_Different_Ways/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pr0f33/zzz_personal/Learning/Javascript/React/How_to_Build_a_React_App-A_Walkthrough_of_the_Many_Different_Ways/node_modules/.pnpm/html-minifier@1.5.0/node_modules/html-minifier/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/React/How_to_Build_a_React_App-A_Walkthrough_of_the_Many_Different_Ways/node_modules/.pnpm/html-minifier@1.5.0/node_modules:/home/pr0f33/zzz_personal/Learning/Javascript/React/How_to_Build_a_React_App-A_Walkthrough_of_the_Many_Different_Ways/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html-minifier/cli.js" "$@"
else
  exec node  "$basedir/../html-minifier/cli.js" "$@"
fi
